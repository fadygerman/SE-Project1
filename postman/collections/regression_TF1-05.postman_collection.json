      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer xyz123_invalid"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/api/v1/cars",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["api", "v1", "cars"]
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Statuscode ist 401 Unauthorized\", function () {",
